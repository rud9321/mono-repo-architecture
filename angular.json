            "styles": [
              "projects/todoy/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/todoy/src/favicon.ico",
              "projects/todoy/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
